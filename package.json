{
  "name": "js-cpa",
  "version": "0.1.0",
  "description": "Code pattern analysis for JS",
  "main": "lib/index.js",
  "bin": {
    "js-cpa": "bin/js-cpa.js"
  },
  "scripts": {
    "test": "jest",
    "start": "webpack --config client/webpack.config.js --watch",
    "build": "NODE_ENV=production webpack --config client/webpack.config.js",
    "dev": "bash -c 'cat <(find ./public/*) <(find ./lib/*) | entr -s \"./bin/js-cpa.js -m -t 1 -R html test/fixtures/*.js\"'"
  },
  "keywords": ["code", "repetition", "pattern", "duplication", "javascript"],
  "author": "Vignesh Shanmugam <vignesh.shanmugam22@gmail.com> (https://vigneshh.in)",
  "contributors": ["Boopathi Rajaa", "Vignesh Shanmugam"],
  "homepage": "https://github.com/vigneshshanmugam/js-cpa",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vigneshshanmugam/js-cpa.git"
  },
  "dependencies": {
    "babel-types": "^7.0.0-alpha.15",
    "babylon": "^7.0.0-beta.18",
    "chalk": "^2.0.1",
    "commander": "^2.11.0",
    "ejs": "^2.5.7"
  },
  "devDependencies": {
    "babel-core": "6.25.0",
    "babel-loader": "7.1.1",
    "babel-minify-webpack-plugin": "^0.2.0",
    "babel-plugin-transform-react-jsx": "6.24.1",
    "babel-preset-env": "1.6.0",
    "classnames": "2.2.5",
    "css-loader": "0.28.4",
    "extract-text-webpack-plugin": "3.0.0",
    "jest": "^20.0.4",
    "mkdirp": "0.5.1",
    "normalize.css": "7.0.0",
    "preact": "8.2.1",
    "prismjs": "1.6.0",
    "style-loader": "0.18.2",
    "webpack": "3.4.1"
  },
  "files": ["bin", "lib", "LICENSE", "README.md"]
}
